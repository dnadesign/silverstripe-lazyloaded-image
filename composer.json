{
    "name": "dnadesign/silverstripe-lazyloaded-image",
    "description": "Implements the LQIP (Low Quality Image Placeholder) pattern powered by lazysizes",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "lazyload",
        "lazysizes",
        "image"
    ],
    "license": "MIT",
    "require": {
        "silverstripe/framework": "^6.0",
        "chrometoaster/silverstripe-image-quality": "dev-silverstripe-6"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dnadesign/silverstripe-image-quality.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "DNADesign\\Images\\": "src/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-lazyloaded-image",
        "expose": [
            "client"
        ]
    }
}
